<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.imzyao.mappers.SysUserMapper">

    <sql id="selectUserSql">
        select `id`,
               `user_name`,
               `pass_word`,
               `nick_name`,
               `avatar`,
               `telephone`,
               `email`,
               `sex`,
               `status`,
               `del_flag`,
               `login_ip`,
               `login_date`,
               `create_by`,
               `create_time`,
               `update_by`,
               `update_time`
        from sys_user
    </sql>


</mapper>
